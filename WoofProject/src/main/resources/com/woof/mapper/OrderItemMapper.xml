<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woof.mapper.OrderItemMapper">

<insert id="addToOrderItem" parameterType="java.util.List">
	<foreach collection="list" item="orderItem" separator=";">
		INSERT INTO orderItem VALUES 
		(#{orderItem.orderHistoryNo}, #{orderItem.itemNo}, #{orderItem.itemName}, #{orderItem.itemQuantity}, 
		#{orderItem.itemPrice}, #{orderItem.itemMainPic})
	</foreach>
</insert>
</mapper>